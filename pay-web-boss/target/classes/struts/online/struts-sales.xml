<struts>
	<package name="sales" extends="base">
		<!-- 销售人员管理Action -->
		<action name="sales_*" class="wusc.edu.pay.web.boss.action.SalesAction" method="{1}">
			<result name="salesList">page/sales/salesList.jsp</result>
